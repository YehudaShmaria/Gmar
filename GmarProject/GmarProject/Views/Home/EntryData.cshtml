
@{
    ViewBag.Title = "EntryData";
}

<div style="text-align:center" class="text-center">
    <h1 style="font-style:italic;font-size:50px" class="text-primary"><u>Entry Data</u></h1>
    <img onload="Chack()" style="height:100px" src="data:image/jpeg;base64," />
    <h2 id="Date"></h2>
    <h2 id="NumOfDay"></h2>

    <div class="container text-right">
        <div class="row">
            <div class="mx-auto">
                <div style="display:none" id="InsertWeight">
                    <div class="form-group text-right">
                        <label for="CurrentWeight">זהו סוף שבוע הגיע הזמן לראות מה המשקל:</label>
                        <input id="CurrentWeight" type="number" placeholder="!!Enter you asnsher" class="form-control form-control-underlined border-danger">
                    </div>
                </div>

                <div style="border-radius:5px" class="col-lg-4 text-right">
                    <h2 class="text-center" style="border:solid; background-color:darkkhaki; border-radius:5px">נתונים חשובים של האתר</h2>
                    <div class="btn-group-vertical">
                        <a href="" class="btn btn-info btn">פרופיל שלי</a>
                        <br />
                        <a href="" class="btn btn-warning btn">מכונה לומדת</a>
                        <br />
                        <a href="" class="btn btn-info btn">סיכום שבועי</a>
                        <br />
                        <a href="" class="btn btn-warning btn">הכנס נתון יומי</a>
                    </div>
                    <img class="img-thumbnail" src="~/Images/Week.jpg" style="height:150px; display:inline-block; margin-right:25px" />
                </div>

                <form class="col-lg-4">
                    <div class="form-group text-right">
                        <label for="Date1">תאריך:</label>
                        <input id="Date1" type="date" placeholder="!!Enter you asnsher" class="form-control form-control-underlined border-danger">
                    </div>
                    <div class="form-group text-right">
                        <label for="Morning">ארוחת בוקר:</label>
                        <input id="Morning" type="text" placeholder="!!Enter you asnsher" class="form-control form-control-underlined border-danger">
                    </div>

                    <div class="form-group text-right">
                        <label for="noon">ארוחת צהריים:</label>
                        <input id="noon" type="text" placeholder="!!Enter you asnsher" class="form-control form-control-underlined border-danger">
                    </div>
                    <div class="form-group text-right">
                        <label for="evening">ארוחת ערב:</label>
                        <input id="evening" type="text" placeholder="!!Enter you asnsher" class="form-control form-control-underlined border-danger">
                    </div>
                    <div class="form-group text-right">
                        <label for="Snack">נישנושים:</label>
                        <input id="Snack" type="text" placeholder="!!Enter you asnsher" class="form-control form-control-underlined border-danger">
                    </div>
                    <div class="form-group mb-4 text-right">
                        <label for="Mood">מצב רוח:</label>
                        <select class="form-control" id="Mood">
                            <option></option>
                            <option>שמח</option>
                            <option>עצבני</option>
                            <option>דיכאון</option>
                        </select>
                    </div>
                    <div class="form-group text-right">
                        <label for="DynamicFigure">הכנס מה שאתה חחושב לנכון (טקסט חופשי)....</label>
                        <input id="DynamicFigure" type="text" placeholder="!!Enter you asnsher" class="form-control form-control-underlined border-danger">
                    </div>
                    <div class="col text-left">
                        <button onclick="SendData()" class="btn btn-primary">שלח נתונים</button>
                    </div>
                </form>

                <div style="border-radius:5px" class="col-lg-4 text-right">
                    <h2 class="text-center" style="border:solid; background-color:darkkhaki; border-radius:5px">נתונים חשובים של האתר</h2>
                    <div class="btn-group-vertical">
                        <a href="" class="btn btn-info btn">פרופיל שלי</a>
                        <br />
                        <a href="" class="btn btn-warning btn">מכונה לומדת</a>
                        <br />
                        <a href="" class="btn btn-info btn">סיכום שבועי</a>
                        <br />
                        <a href="" class="btn btn-warning btn">הכנס נתון יומי</a>
                    </div>
                    <img class="img-thumbnail" src="~/Images/Week.jpg" style="height:150px; display:inline-block; margin-right:25px" />
                </div>
            </div>
        </div>
    </div>

</div>
<script>
    let today = new Date();
    function Chack() {
        if (today.getDay() == 2) {
            document.getElementById("InsertWeight").style.display = "inline";
        }
    }

    Chack();
   
    document.getElementById("Date").innerHTML = today.toLocaleDateString();
    document.getElementById("NumOfDay").innerHTML = today.getDay()+1;
    function SendData() {
        var User = "@HttpContext.Current.User.Identity.Name";
        var obj = {
            Name: User,
            Day: {
                Date: document.getElementById("Date1").value,
                NumOfDay: today.getDay() + 1,
                Morning: document.getElementById("Morning").value,
                noon: document.getElementById("noon").value,
                evening: document.getElementById("evening").value,
                Snack: document.getElementById("Snack").value,
                Mood: document.getElementById("Mood").value,
                DynamicFigure: document.getElementById("DynamicFigure").value
            },
            CurrentWeight: document.getElementById("CurrentWeight").value
        }
        const json = JSON.stringify(obj);
        const xml = new XMLHttpRequest();
        xml.open('POST', 'http://localhost:3000/getdata')
        xml.setRequestHeader("Content-type", "application/json");
        xml.send(json);
        alert('נתונים נשלחו בהצלחה!!!')
    }
</script>

